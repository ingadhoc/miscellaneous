<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="okr_application">
            <field name="name">OKR Management</field>
        </record>

        <record id="group_okr_user" model="res.groups">
            <field name="name">OKR User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="okr_application"/>
        </record>
        <record id="group_okr_manager" model="res.groups">
            <field name="name">OKR Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_okr_user'))]"/>
            <field name="category_id" ref="okr_application"/>
        </record>
    </data>
</odoo>
